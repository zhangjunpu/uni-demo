{
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "uni-app"
      }
    },
    {
      "path": "pages/custom-navbar/index",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTextStyle": "#fff"
      }
    },
    {
      "path": "pages/animate/index",
      "style": {
        "navigationBarTitleText": "animate",
        "disableScroll": true
      }
    },
    {
      "path": "pages/rating/index",
      "style": {
        "navigationBarTitleText": "rating"
      }
    },
    {
      "path": "pages/left-load/index",
      "style": {
        "navigationBarTitleText": "left-load",
        "disableScroll": true,
        "app-plus": {
          "bounce": "none"
        }
      }
    },
    {
      "path": "pages/scroll/index",
      "style": {
        "navigationBarTitleText": "index",
        "disableScroll": true,
        "app-plus": {
          "bounce": "none"
        }
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
